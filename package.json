{
  "name": "vue-boilerplate",
  "version": "0.0.1",
  "description": "My custom Vue.js boilerplate",
  "main": "index.js",
  "repository": "git@github.com:dennybiasiolli/vue-boilerplate.git",
  "author": "Denny Biasiolli <denny.biasiolli@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "webpack --config=build/webpack.dev.conf.js",
    "dist": "webpack --config=build/webpack.prod.conf.js",
    "lint": "eslint src build --ext .js,.vue --fix",
    "start": "webpack-dev-server --config build/webpack.dev.conf.js",
    "test": "jest --config test/unit/jest.conf.js --coverage"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.28",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "eslint-plugin-vue": "^4.5.0",
    "jest": "^23.6.0",
    "jest-vue-preprocessor": "^1.4.0",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "vue": "^2.5.22",
    "vue-loader": "^15.5.1",
    "vue-router": "^3.0.2",
    "vue-template-compiler": "^2.5.22",
    "vuetify": "^1.4.2",
    "vuex": "^3.0.1",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.3"
  }
}
