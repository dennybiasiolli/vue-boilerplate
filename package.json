{
  "name": "vue-boilerplate",
  "version": "0.0.1",
  "description": "My custom Vue.js boilerplate",
  "main": "index.js",
  "repository": "git@github.com:dennybiasiolli/vue-boilerplate.git",
  "author": "Denny Biasiolli <denny.biasiolli@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "webpack --config=build/webpack.dev.conf.js",
    "dist": "webpack --config=build/webpack.prod.conf.js",
    "lint": "eslint src build --ext .js,.vue --fix",
    "start": "webpack-dev-server --config build/webpack.dev.conf.js",
    "test": "jest --config test/unit/jest.conf.js --coverage"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.11",
    "babel-preset-env": "^1.6.1",
    "css-loader": "^0.28.9",
    "eslint": "^4.16.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^4.2.2",
    "jest": "^22.1.4",
    "jest-vue-preprocessor": "^1.3.1",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "vue": "^2.5.13",
    "vue-loader": "^13.7.0",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.13",
    "vuex": "^3.0.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1"
  }
}
